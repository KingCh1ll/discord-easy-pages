{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["README.md", "src"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": false
        }
    },
    "opts": {
        "destination": "../discordeasypages.github.io/",
        "readme": "README.md",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "template": "node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "title": "DiscordEasyPages",
            "theme": "dark",
            "filter": false,
            "menu": [{
                    "title": "NPM",
                    "link": "https://www.npmjs.com/package/discordeasypages",
                    "target": "_blank"
                },
                {
                    "title": "GitHub",
                    "link": "https://github.com/kingch1ll/DiscordEasyPages/",
                    "target": "_blank"
                }
            ],
            "meta": [{
                    "name": "author",
                    "content": "KingCh1ll"
                },
                {
                    "name": "description",
                    "content": "An effective & easy-to-use package for making discord.js pages."
                },
                {
                    "itemprop": "name",
                    "content": "DiscordEasyPages"
                },
                {
                    "itemprop": "image",
                    "content": ""
                },
                {
                    "property": "og:url",
                    "content": "https://DiscordEasyPages.org"
                },
                {
                    "property": "og:type",
                    "content": "website"
                },
                {
                    "property": "og:title",
                    "content": "DiscordEasyPages"
                },
                {
                    "property": "og:description",
                    "content": "An effective & easy-to-use package for making discord.js pages."
                },
                {
                    "property": "og:image",
                    "content": ""
                }
            ],
            "create_style": "::-webkit-scrollbar { display: none }",
            "add_script_path": [{
                "src": "https://arc.io/widget.min.js#2srirM1z"
            }],
            "add_style_path": [{
                "rel": "shortcut icon",
                "type": "image/png",
                "href": ""
            }],
            "search": true,
            "footer": "Powered by <a href='https://js.org' target='_blank'>js.org</a>, <a href='https://jsdoc.app/' target='_blank'>JSDoc</a>, <a href='https://github.com/ankitskvmdam/clean-jsdoc-theme' target='_blank'>clean-jsdoc-theme</a> and Love."
        }
    }
}